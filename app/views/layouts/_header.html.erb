<div id="header">
  <div id="logo"><a href="#">ClickStarter</a></div>
  <span id="discover"><a href="#">Discover</a></span>
	<% if logged_in? %>
	  <!-- <%= current_user.email %> -->
	  <span id="start"><a href="#/projects/new">Start</a></span>
	  <div class="session-control" id="sign-out">
	    <%= button_to "Sign Out", session_url, :method => :delete %>
      </div>
	  <div id="user-dropdown">
	    Me
	  </div>
	<% else %>
      <div class="session-control" id="sign-in">
        <%= link_to "Sign In", new_session_url %>
      </div>
      <div class="session-control" id="sign-up">
        <%= link_to "Sign Up", new_user_url %>
      </div>
	<% end %>
</div>